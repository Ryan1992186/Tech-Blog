<h4 id="indiv-post-header">{{postData.title}}</h4>
<p>{{postData.content}}</p>

<div class="comment-container">
{{#each commentData as |comment|}}
        <p class="comment-content">{{content}}</p>
{{/each}}
</div>

<form id="comment-message">
<input type="text" id="comment-text" name="text"><br>
<input type="submit" id="send-comment-btn" name="send-msg-btn">
</form>
<script src ="/js/comment.js"></script>